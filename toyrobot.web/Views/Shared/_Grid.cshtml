@model toyrobot.web.Models.HomePageViewModel

<div id="container" class="col-lg-8">
    @for (var i = 0; i < Model.Rows; i++)
    {
        <div class="trow">
            @for (var j = 0; j < Model.Columns; j++)
            {
                <div class="box" id="bx_@(i + "_" + j)"></div>
            }
        </div>
    }

    <div id="robotimg" style="display: none;"></div>
</div>
